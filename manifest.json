{
    "manifest_version": 2,
    "name": "Bookmark Tagger",
    "description": "Adds bookmark with tags retrieved from the bookmarked page",
    "version": "1.0.0",
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_title": "BookmarkTagger",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}